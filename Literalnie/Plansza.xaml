            <TextBox x:Name="GuessTextBox5" Grid.Column="4" Grid.Row="0" Margin="5" FontSize="24" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" PreviewTextInput="TextBox_PreviewTextInput"/>

