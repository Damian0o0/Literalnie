        <Button Content="Check Guess" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,50" Click="CheckGuess_Click"/>
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Top" Width="193" FontSize="35px" FontWeight="Bold" FontFamily="sans-serif;" Height="48" Margin="0,20,0,0">LITERALNIE</TextBlock>
