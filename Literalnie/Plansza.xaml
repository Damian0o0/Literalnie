            <TextBox x:Name="GuessTextBox11" Grid.Column="0" Grid.Row="2" Margin="5" FontSize="24" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" PreviewTextInput="TextBox_PreviewTextInput"/>
