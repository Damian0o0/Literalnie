        <Button Name="Statystyki" BorderBrush="Transparent" Click="Statystyki_Click" Margin="10,12,623,716" RenderTransformOrigin="0.908,1.148">
            <Button.Background>
                <ImageBrush ImageSource="statystyki.jpg"/>
            </Button.Background>
        </Button>
        <Button Name="Github" BorderBrush="Transparent" Click="Github_Click" Margin="82,12,551,716" RenderTransformOrigin="0.908,1.148">
            <Button.Background>
                <ImageBrush ImageSource="github.png"/>
            </Button.Background>
        </Button>
        <Button Name="Ustawienia" BorderBrush="Transparent" Click="Ustawienia_Click" Margin="551,8,82,720" RenderTransformOrigin="0.908,1.148">
